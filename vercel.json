{
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": "npm run start:dev",
  "installCommand": "npm install --legacy-peer-deps",
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts"
    }
  ]
}