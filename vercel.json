{
  "version": 2,
  "buildCommand": "pnpm build",
  "devCommand": "pnpm start:dev",
  "installCommand": "pnpm install --legacy-peer-deps",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "HEAD", "OPTIONS"]
    }
  ]
}
